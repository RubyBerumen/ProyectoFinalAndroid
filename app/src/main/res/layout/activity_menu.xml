<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_altas"
        android:layout_width="117dp"
        android:layout_height="88dp"
        android:layout_gravity="start"
        android:layout_marginStart="76dp"
        android:layout_marginTop="136dp"
        android:enabled="false"
        android:text="@string/btnAltas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_bajas"
        android:layout_width="117dp"
        android:layout_height="88dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"
        android:layout_marginTop="136dp"
        android:enabled="false"
        android:text="@string/btnBajas"
        app:layout_constraintStart_toEndOf="@+id/btn_altas"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_cambios"
        android:layout_width="117dp"
        android:layout_height="88dp"
        android:layout_gravity="start"
        android:layout_marginStart="76dp"
        android:layout_marginTop="64dp"
        android:enabled="false"
        android:text="@string/btnCambios"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_altas" />

    <Button
        android:id="@+id/btn_consultas"
        android:layout_width="117dp"
        android:layout_height="88dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"
        android:layout_marginTop="64dp"
        android:enabled="false"
        android:text="@string/btnConsultas"
        app:layout_constraintStart_toEndOf="@+id/btn_cambios"
        app:layout_constraintTop_toBottomOf="@+id/btn_bajas" />

    <Button
        android:id="@+id/btn_salir"
        android:layout_width="163dp"
        android:layout_height="57dp"
        android:layout_gravity="start"
        android:layout_marginStart="132dp"
        android:layout_marginBottom="172dp"
        android:enabled="false"
        android:text="@string/btnSalirMenu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>